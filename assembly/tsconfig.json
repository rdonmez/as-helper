{
  "extends": "assemblyscript/std/assembly.json",
  "include": [
    "./**/*.js",
    "./**/*.ts"
  ],
  "compilerOptions": {
    "target": "es5",   
    "module": "commonjs", 
    "downlevelIteration": true,  
    "strict": true, 
    "esModuleInterop": true,    
    "resolveJsonModule": true,
    "skipLibCheck": true, 
    "forceConsistentCasingInFileNames": true ,
   
    "allowJs": true,
    "checkJs": false
  }
}